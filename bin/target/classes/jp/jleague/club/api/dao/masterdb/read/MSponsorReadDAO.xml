<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MSponsorReadDAO">
	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MSponsorDTO" id="MSponsorDTO">
		<result column="sponsor_serial_number" property="sponsorSerialNumber" javaType="java.lang.Integer" />		
		<result column="sponsor_id" property="sponsorId" javaType="java.lang.Integer" />				
		<result column="sponsor_name" property="sponsorName" javaType="java.lang.String" />	
		<result column="sponsor_name_roughly" property="sponsorNameRoughly" javaType="java.lang.String" />
		<result column="sponsor_image_path" property="sponsorImagePath" javaType="java.lang.String" />	
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />	
	</resultMap>
	
	<select id="selectBySponsorId" resultMap="MSponsorDTO" parameterType="java.lang.Integer">
		SELECT
			sponsor_serial_number,
			sponsor_id,
			sponsor_name,
			sponsor_name_roughly,
			sponsor_image_path,
			regist_datetime
		FROM
			m_sponsor
		WHERE 
			sponsor_id = #{sponsorId}
		ORDER BY
		sponsor_id ASC; 
	</select>
</mapper> 
