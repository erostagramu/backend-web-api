<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.write.IMeProgramWriteDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.IMeProgramDTO" id="IMeProgramDTO">
		<result column="member_id" property="memberId" javaType="java.lang.String" />
		<result column="program_code" property="programCode" javaType="java.lang.String" />
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />
		<result column="update_datetime" property="updateDatetime" javaType="java.util.Date" />
	</resultMap>

	<insert id="insertIMeProgram" parameterType="jp.jleague.club.api.dao.masterdb.dto.IMeProgramDTO">
		INSERT INTO
			i_me_program
				(member_id,
			program_code,
			regist_datetime,
			update_datetime)
			values(
				#{memberId},
				#{programCode},
				#{registDatetime},
				#{updateDatetime}
				)
	</insert>

	<select id="selectByMemberId" parameterType="java.lang.String" resultMap="IMeProgramDTO">
		select member_id,
			program_code,
			regist_datetime,
			update_datetime
		from i_me_program
		where member_id = #{memberId}
	</select>

</mapper>
