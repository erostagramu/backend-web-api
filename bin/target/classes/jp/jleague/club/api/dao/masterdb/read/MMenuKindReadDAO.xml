<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MMenuKindReadDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MMenuKindDTO" id="MMenuKindDTO">
		<result column="menu_kind_id" property="menuKindId" javaType="java.lang.Integer" />
		<result column="menu_kind_name" property="menuKindName" javaType="java.lang.String" />
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />
	</resultMap>

	<select id="selectAllOrderByPrimaryKey" resultMap="MMenuKindDTO">
		select menu_kind_id,
			   menu_kind_name,
			   regist_datetime
		  from m_menu_kind
	  order by menu_kind_id
	</select>

</mapper>
