<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.CAwardsReadDAO">
	
	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.CAwardsDTO" id="dto">
		<result column="specific_magnification" property="specificMagnification" />
	</resultMap>
	
	<select id="findBy" resultMap="dto" >
		SELECT
			specific_magnification
		FROM
			c_awards
		WHERE
			config_type = #{type}
		;
	</select>
</mapper> 
