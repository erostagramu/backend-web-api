<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MMenuReadDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MMenuDTO" id="MMenuDTO">
		<result column="menu_id" property="menuId" javaType="java.lang.Integer" />
		<result column="menu_kind_id" property="menuKindId" javaType="java.lang.Integer" />
		<result column="menu_name" property="menuName" javaType="java.lang.String" />
		<result column="menu_url" property="menuUrl" javaType="java.lang.String" />
		<result column="external_browser" property="externalBrowser" javaType="java.lang.Integer" />
		<result column="session_required_flg" property="sessionRequiredFlg" javaType="java.lang.Integer" />
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />
	</resultMap>

	<select id="selectByMenuKindId" resultMap="MMenuDTO" parameterType="java.lang.Integer">
		select menu_id,
			   menu_kind_id,
			   menu_name,
			   menu_url,
			   external_browser,
			   session_required_flg,
			   regist_datetime
		  from m_menu
		 where menu_kind_id = #{menuKindId}
		 order by row_order
	</select>

</mapper>
