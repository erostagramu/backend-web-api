<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.CServerMaintenanceReadDAO">

	<resultMap id="CServerMaintenanceDTO" type="jp.jleague.club.api.dao.masterdb.dto.CServerMaintenanceDTO">
		<result property="underMaintenanceFlg" column="under_maintenance_flg"/>
		<result property="registDatetime" column="regist_datetime"/>
		<result property="updateDatetime" column="update_datetime"/>
	</resultMap>
	
	<select id="findBy" resultMap="CServerMaintenanceDTO" >
		SELECT
			under_maintenance_flg,
			regist_datetime,
			update_datetime
		FROM
			c_server_maintenance
		WHERE
			config_type = #{type}
		;
	</select>

</mapper> 
