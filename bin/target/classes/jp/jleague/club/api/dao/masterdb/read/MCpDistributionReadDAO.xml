<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MCpDistributionReadDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MCpDistributionDTO" id="MCpDistributionTO">
		<id column="distribution_src_id" property="distributionSrcId" />
		<result column="distribution_src_kind" property="distributionSrcKind"  />
		<result column="distribution_src_name" property="distributionSrcName"  />
		<result column="regist_datetime" property="registDatetime"  />
	</resultMap>
		
	<select id="selectOneByDistributionSrcId" resultMap="MCpDistributionTO" >
		SELECT
			distribution_src_id,
			distribution_src_kind,
			distribution_src_name,
			regist_datetime 
		FROM
			m_cp_distribution 
		WHERE
			distribution_src_id = #{distributionSrcId}
		;
		
	</select>
</mapper> 
