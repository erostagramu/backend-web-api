<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MStadiumNaviReadDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MStadiumNaviDTO" id="MStadiumNaviDTO">
		<result column="venue_navi_id" property="venueNaviId" javaType="java.lang.Integer" />
		<result column="venue_code" property="venueCode" javaType="java.lang.String" />
		<result column="club_id" property="clubId" javaType="java.lang.String" />
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />
	</resultMap>

	<select id="selectByForeignKey" resultMap="MStadiumNaviDTO" parameterType="jp.jleague.club.api.dao.masterdb.dto.MStadiumNaviDTO">
		select venue_navi_id,
			   venue_code,
			   club_id,
			   regist_datetime
		  from m_stadium_navi
		 where venue_code = #{venueCode}
		   and club_id = #{clubId}
	</select>

</mapper>
