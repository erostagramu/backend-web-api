<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.ICpApplicantInfoReadDAO">

	<select id="isExistBy" resultType="java.lang.Boolean" parameterType="jp.jleague.club.api.dao.masterdb.dto.ICpApplicantInfoDTO">
		SELECT
			COUNT(*) > 0
		FROM
			i_cp_applicant_info 
		<where>
			<if test="cpNoticeId != null">
				cp_notice_id = #{cpNoticeId}
			</if>
			<if test="cpNoticeId != null">
				AND applicant_member_id = #{applicantMemberId}
			</if>
		</where>
		;
	</select>
</mapper> 
