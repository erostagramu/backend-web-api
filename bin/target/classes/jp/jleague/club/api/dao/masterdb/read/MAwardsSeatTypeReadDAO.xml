<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MAwardsSeatTypeReadDAO">

	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MAwardsSeatTypeDTO" id="DTO">
		<result column="seat_type" property="seatType" javaType="java.lang.String" />
		<result column="seat_name" property="seatName" javaType="java.lang.String" />
		<result column="regist_datetime" property="registDatetime" javaType="java.util.Date" />
	</resultMap>

	<select id="selectAll" resultMap="DTO">
		select seat_type
		     , seat_name
		     , regist_datetime
		from m_awards_seat_type
		order by seat_type
	</select>

</mapper>
