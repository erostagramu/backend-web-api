<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MNewsProviderReadDAO">
	
	<resultMap type="jp.jleague.club.api.dao.masterdb.dto.MNewsProviderDTO" id="MNewsProviderDTO">
		<result column="provider_id" property="providerId" javaType="java.lang.Integer" />
		<result column="acquisition_destination_category" property="aquisitionDestinationCategory" javaType="java.lang.String" />
		<result column="provider_source" property="providerSource" javaType="java.lang.String" />
	</resultMap>
	
	<select id="selectByProviderId" resultMap="MNewsProviderDTO" parameterType="java.lang.Integer">
		select provider_id,
		acquisition_destination_category,
		provider_source
		from m_news_provider
		where provider_id = #{providerId}
	</select>

</mapper> 