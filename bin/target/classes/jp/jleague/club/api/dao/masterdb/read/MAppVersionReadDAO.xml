<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.MAppVersionReadDAO">

	<select id="selectUpdateFlg" resultType="java.lang.Integer" parameterType="java.util.Map">
		SELECT update_flg
		FROM m_app_version
		WHERE os_type = #{osType}
		AND version = #{version}
	</select>
</mapper>
