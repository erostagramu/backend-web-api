<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.jleague.club.api.dao.masterdb.read.IMeOtpIdReadDAO">

<resultMap id="IMeOtpIdDTO" type="jp.jleague.club.api.dao.masterdb.dto.IMeOtpIdDTO">
<result javaType="java.lang.String" property="memberId" column="member_id"/>
<result javaType="java.lang.String" property="otpId" column="otp_id"/>
<result javaType="java.lang.String" property="otpClubId" column="otp_club_id"/>
<result javaType="java.lang.String" property="fcMemberGradeCode" column="fc_member_grade_code"/>
<result javaType="java.lang.String" property="fcMemberGradeName" column="fc_member_grade_name"/>
<result javaType="java.util.Date" property="fcGradeExpirationDatetime" column="fc_grade_expiration_datetime"/>
<result javaType="java.util.Date" property="startExpirationDate" column="start_expiration_date"/>
<result javaType="java.util.Date" property="endExpirationDate" column="end_expiration_date"/>
<result javaType="java.util.Date" property="registDatetime" column="regist_datetime"/>
<result javaType="java.util.Date" property="updateDatetime" column="update_datetime"/>
</resultMap>

<select id="selectList" parameterType="jp.jleague.club.api.dao.masterdb.dto.IMeOtpIdDTO" resultMap="IMeOtpIdDTO">
select 
	member_id,
	otp_id,
	otp_club_id,
	fc_member_grade_code,
	fc_member_grade_name,
	fc_grade_expiration_datetime,
	start_expiration_date,
	end_expiration_date,
	regist_datetime,
	update_datetime
from i_me_otp_id 
where member_id = #{memberId} 
<if test="otpId != null">
 and otp_id = #{otpId}
</if>
</select>
</mapper> 
